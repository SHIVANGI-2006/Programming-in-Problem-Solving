class Car:
	def __init__(self, brand, price, model, color):
		self.brand = brand
		self.price = price
		self.model = model
		self.color = color

	def display_details(self):
		print(self.brand)
		print(self.price)
		print(self.model)
		print(self.color)

class Car1(Car):
		def __init__(self, brand, price, model, color):
			super().__init__(brand, price, model, color)

def display_details(self):
		print(f"{self.brand}")
		print(f"{self.price}")
		print(f"{self.model}")
		print(f"{self.color}")

class Car2(Car):
	def __inti__(self, brand, price, model, color):
		super().__inti__(brand, price, model, color)

		def display_details(self):
			print(f"{self.brand}")
			print(f"{self.price}")
			print(f"{self.model}")
			print(f"{self.color}")


def get_car_details():
	brand = input("brand: ")
